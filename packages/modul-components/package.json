{
    "name": "@ulaval/modul-components",
    "version": "1.0.0-beta.131",
    "description": "MODUL vueJS Component library",
    "author": "Martin Simard <martin.simard@dti.ulaval.ca>",
    "homepage": "https://ulaval.github.io/modul",
    "private": false,
    "scripts": {
        "build": "tsc --project tsconfig.lib.json && copyfiles -u 1 \"./src/**/*.json\" dist && copyfiles -u 1 \"./src/**/*.html\" dist && copyfiles -u 1 \"./src/**/*.scss\" dist && copyfiles -u 1 \"./src/**/*.css\" dist && yarn run assetsToDist && yarn run metagen",
        "buildlib": "webpack --config conf/webpack.config.js --colors --env.lib",
        "assetsToDist": "copyfiles -u 1 \"./src/assets/**/*\" dist",
        "start": "webpack --config conf/webpack.config.js --env.openshift && http-server \"./dist\" -p 8080 -a 0.0.0.0",
        "test": "jest --verbose",
        "test:update": "jest --verbose --silent --updateSnapshot",
        "unitwatch": "jest --watch --verbose --silent",
        "metagen": "meta-generator --dest ./dist --package package.json --tsconfig tsconfig.json",
        "test:ci": "jest --ci -w 1 --silent --coverage --testResultsProcessor=./tests/jest/ci-test-processor",
        "lint": "tsc --noEmit && tslint --project tsconfig.json -s ../../node_modules/custom-tslint-formatters/formatters -t grouped ",
        "lint:ci": "tsc --noEmit && tslint --project tsconfig.json -t junit -o reports/junit/js-lint-results.xml ",
        "prepare": "yarn run build",
        "stats": "webpack --config conf/webpack.config.js --env.lib --env.silent --colors --profile --json > reports/stats.json && webpack-bundle-analyzer reports/stats.json",
        "print_version_win": "echo %npm_package_version%",
        "print_version_nx": "echo $npm_package_version",
        "file-gen-cli": "./node_modules/.bin/file-gen-cli"
    },
    "jest-junit": {
        "output": "reports/junit/js-test-results.xml"
    },
    "jest-html-reporter": {
        "outputPath": "reports/junit/js-test-results.html",
        "includeFailureMsg": true
    },
    "dependencies": {
        "axios": "^0.19.0",
        "cleave.js": "^1.5.0",
        "clipboard": "^2.0.4",
        "css-element-queries": "^0.4.0",
        "enquire.js": "^2.1.6",
        "es6-promise": "^4.1.1",
        "filesize": "^3.6.0",
        "froala-editor": "3.0.5",
        "jquery": "^3.3.1",
        "libphonenumber-js": "^1.7.19",
        "mobile-drag-drop": "^2.3.0-rc.1",
        "moment": "^2.19.3",
        "popper.js": "^1.12.9",
        "portal-vue": "~1.1.1",
        "sprintf-js": "^1.1.1",
        "vue-touch": "^2.0.0-beta.4",
        "zingtouch": "^1.0.6"
    },
    "peerDependencies": {
        "vue": "^2.6.4",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "@types/cleave.js": "^1.4.0",
        "@types/filesize": "^3.6.0",
        "@types/googlemaps": "^3.37.0",
        "@types/sprintf-js": "^1.1.0",
        "@types/webpack-env": "^1.13.6",
        "@vue/test-utils": "^1.0.0-beta.29",
        "autoprefixer": "^9.4.7",
        "css-loader": "^2.1.0",
        "file-gen-cli": "^1.2.2",
        "file-loader": "^3.0.1",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-html-reporter": "^2.6.2",
        "jest-junit": "^8.0.0",
        "jest-serializer-vue": "^2.0.2",
        "meta-generator": "^1.0.2-beta.1",
        "node-sass": "^4.11.0",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.3.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-scss": "^3.1.3",
        "stylelint-webpack-plugin": "^0.10.5",
        "svg-inline-loader": "^0.8.0",
        "thread-loader": "^2.1.2",
        "ts-jest": "^24.1.0",
        "ts-loader": "^5.3.3",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "^1.1.2",
        "vue": "^2.6.10",
        "vue-class-component": "^6.1.1",
        "vue-property-decorator": "^6.0.0",
        "vue-router": "^3.0.1",
        "vue-server-renderer": "^2.6.10",
        "vue-template-loader": "^1.0.0",
        "webpack": "^4.29.3",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.1.14"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ulaval/modul.git"
    },
    "bugs": {
        "url": "https://github.com/ulaval/modul/issues"
    },
    "files": [
        "src/**/*.ts",
        "dist"
    ],
    "license": "Apache-2.0"
}
