{
    "name": "storybook",
    "version": "1.2.0-RC.0",
    "description": "modUL storybook",
    "author": "Charles Maheu <charles.maheu@dti.ulaval.ca>",
    "private": true,
    "scripts": {
        "storybook": "start-storybook --config-dir ./conf/storybook --port 8082 --static-dir ./public",
        "storybook:openshift": "start-storybook --ci --port 8080 --config-dir ./conf/storybook --static-dir ./public",
        "test:ci": "jest -w 1 --ci --silent --config ./conf/storybook/jest.config.storybook.js --verbose",
        "test": "jest --config ./conf/storybook/jest.config.storybook.js --verbose --silent",
        "test:update": "jest --config ./conf/storybook/jest.config.storybook.js --verbose --updateSnapshot",
        "lint": "tsc --noEmit && tslint --project tsconfig.json -s ../../node_modules/custom-tslint-formatters/formatters -t grouped ",
        "lint:ci": "tsc --noEmit && tslint --project tsconfig.json -t junit -o reports/junit/js-lint-results.xml",
        "build": "build-storybook --config-dir ./conf/storybook --output-dir dist --static-dir ./public"
    },
    "dependencies": {
        "vue": "^2.6.10",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@storybook/addon-a11y": "^5.3.17",
        "@storybook/addon-actions": "^5.3.17",
        "@storybook/addon-backgrounds": "^5.3.17",
        "@storybook/addon-knobs": "^5.3.17",
        "@storybook/addon-notes": "^5.3.17",
        "@storybook/addon-storyshots": "^5.3.17",
        "@storybook/addon-viewport": "^5.3.17",
        "@storybook/vue": "^5.3.17",
        "@types/storybook__addon-storyshots": "^5.3.1",
        "@types/webpack-env": "^1.15.1",
        "acorn": "^7.1.1",
        "autoprefixer": "^9.7.4",
        "babel-loader": "^8.0.5",
        "babel-plugin-dynamic-import-node": "^2.2.0",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-vue": "^2.0.2",
        "copyfiles": "^1.2.0",
        "css-loader": "^2.1.0",
        "custom-tslint-formatters": "^2.4.0",
        "file-loader": "^3.0.1",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "git-revision-webpack-plugin": "^3.0.4",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-html-reporter": "^2.8.1",
        "jest-junit": "^8.0.0",
        "jest-serializer-vue": "^2.0.2",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.3.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-scss": "^3.15.0",
        "stylelint-webpack-plugin": "^0.10.5",
        "svg-inline-loader": "^0.8.2",
        "thread-loader": "^2.1.2",
        "ts-jest": "^24.1.0",
        "ts-loader": "^5.3.3",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "^1.1.2",
        "vue-class-component": "^7.2.3",
        "vue-loader": "^15.7.0",
        "vue-property-decorator": "^8.4.1",
        "vue-router": "^3.0.1",
        "vue-server-renderer": "^2.5.23",
        "vue-template-compiler": "^2.5.23",
        "vue-template-loader": "^1.0.0",
        "webpack": "^4.42.0",
        "webpack-bundle-analyzer": "^3.6.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
